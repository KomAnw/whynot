{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "testMatch": ["<rootDir>/src/**/*.test.{ts,tsx}"],
  "moduleNameMapper": {
    "^src(.*)$": "<rootDir>/src$1",
    "^components(.*)$": "<rootDir>/src/components$1"
  },
  "transform": {
    ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub"
  }
}
